page {
    width: 100%;
    height: 100%;
    background: rgb(45, 68, 134)
}

.conter {
    width: 100%;
    height: 100%;
    background-image: url(https://www.nuochewang.net/wximages/index_bg.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
}

.big-text {
    width: 100%;
    height: 100rpx;
    padding: 30rpx 0 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.big-text image {
    width: 75%;
    height: 100%
}

.num {
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.numItme {
    height: 100%;
    width: 70rpx;
    position: relative
}

.numItme image {
    height: 100%;
    width: 70rpx;
}

.numItme .num-text {
    height: 100%;
    width: 60rpx;
    position: absolute;
    top: 0;
    z-index: 10000;
    display: flex;
    justify-content: center;
    align-items: center;
}

.num-text text {
    color: #ffed37;
    font-size: 70rpx;
    font-weight: 700;
}

.car-owner {
    width: 100%;
    height: 120rpx;
    line-height: 120rpx;
    color: #FFFFFF;
    font-size: 60rpx;
    text-align: center;
}

.btn {
    width: 100%;
    height: 120rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 10rpx 90rpx;
    box-sizing: border-box;
    z-index: 10000;
}

.btn button {
    width: 100%;
    background: #ed483f;
    border-radius: 50rpx;
    color: #FFFFFF;
    font-size: 40rpx;
    height: 100rpx;
    line-height: 100rpx
}

.experience {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
}

.item-bg {
    /* background: rgba(44, 67, 135, 0);
    border: 1rpx solid rgba(44, 67, 135, 0); */
    color: #FFFFFF;
    font-size: 35rpx;
    text-align: center;
    text-decoration: underline;
    background: none
}

.item-bg::after {
    border: none;
    outline: none;
}

.flow {
    width: 100%;
    height: 250rpx;
    padding: 0 40rpx;
    box-sizing: border-box
}

.box {
    width: 100%;
    height: 100%;
    background: rgba(255, 255, 255, .7);
    border-radius: 10rpx;
}

.top {
    height: 70rpx;
    width: 90%;
    margin-left: 5%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    color: #31518c;
    font-size: 32rpx
}

.line {
    background: #31518c;
    display: inline-block;
    height: 1rpx;
    overflow: hidden;
    vertical-align: middle;
    flex-basis: 35%;
}

.content {
    height: 160rpx;
    width: 90%;
    margin-left: 5%;
}

.content image {
    height: 160rpx;
    width: 100%;
}

.yellow-text {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    color: #fff100;
    font-size: 34rpx;
}

.explain {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    color: #abbabc;
    font-size: 28rpx;
    text-align: center
}